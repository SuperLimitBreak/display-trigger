{
  "parser": "babel-eslint",
  "plugins": [
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "no-shadow": 1,
    "array-callback-return": 1,
    "no-else-return": 1,
    "eqeqeq": "warn",
    "consistent-return": 1,
    "padded-blocks": [1, "never"],
    "indent": [1, 4, {"SwitchCase": 1}],
    "camelcase": 1,
    "prefer-const": 1,
    "semi": 1,
    "comma-dangle": [1, "always-multiline"],
    "quotes": [ 1, "single" ],
    "no-undef": 1,
    "global-strict": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0
  }
}
