{
  "name": "displayTrigger",
  "version": "0.0.1",
  "description": "Display video/etc from 'trigger events' from a remote system",
  "main": "app.js",
  "dependencies": {
    "core-js": "^2.0.0",
    "normalize.css": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-polyfill": "latest",
    "babel-preset-modern": "latest",
    "babelify": "latest",
    "copyfiles": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-loader": "latest",
    "eslint-plugin-native-over-lodash": "latest",
    "file-loader": "latest",
    "glob": "latest",
    "jasmine-core": "latest",
    "jsx-loader": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-firefox-launcher": "latest",
    "karma-jasmine": "latest",
    "karma-sourcemap-loader": "latest",
    "karma-spec-reporter": "latest",
    "karma-webpack": "latest",
    "minimist": "latest",
    "node-sass": "latest",
    "npm-run-all": "latest",
    "null-loader": "latest",
    "open": "latest",
    "pycollections": "latest",
    "rimraf": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest",
    "webpack-validator": "latest"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "scripts": {
    "start": "webpack-dev-server",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "validate": "npm-run-all --parallel validate-webpack:* lint test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:dist": "webpack-validator webpack.config.js --env.dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rimraf dist/*"
  },
  "repository": "git@github.com:SuperLimitBreak/displayTrigger.git",
  "keywords": [
    "displayTrigger",
    "html5",
    "projector",
    "midi"
  ],
  "author": "superlimitbreak.uk",
  "license": "GPL3",
  "private": true
}
