                "deviceid": "rear",
                "func": "gsap.start",
                "elements": {
                    "logo": {"src": "logo/superLimitBreak_logo.svg", "height": "1vh", "className": "center"}
                },
                "gsap_timeline": [
                    ["logo", "to", "element::logo", 0, {"opacity": 0}],
                    ["logo", "to", "element::logo", 2, {"opacity": 1}]
                ]
            }
        ]
    },


